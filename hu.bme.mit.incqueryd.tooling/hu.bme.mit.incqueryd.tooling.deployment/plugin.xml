<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="hu.bme.mit.incqueryd.tooling.deployment.commands.install"
            name="Install IncQuery-D">
      </command>
      <command
            id="hu.bme.mit.incqueryd.tooling.deployment.commands.deploy"
            name="Deploy IncQuery-D">
      </command>
      <command
            id="hu.bme.mit.incqueryd.tooling.deployment.commands.start"
            name="Start IncQuery-D">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="hu.bme.mit.incqueryd.tooling.deployment.commands.install"
               style="push">
         </command>
         <command
               commandId="hu.bme.mit.incqueryd.tooling.deployment.commands.deploy"
               style="push">
         </command>
         <command
               commandId="hu.bme.mit.incqueryd.tooling.deployment.commands.start"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="hu.bme.mit.incqueryd.tooling.deployment.InstallArchitectureHandler"
            commandId="hu.bme.mit.incqueryd.tooling.deployment.commands.install">
      </handler>
      <handler
            class="hu.bme.mit.incqueryd.tooling.deployment.DeployArchitectureHandler"
            commandId="hu.bme.mit.incqueryd.tooling.deployment.commands.deploy">
      </handler>
      <handler
            class="hu.bme.mit.incqueryd.tooling.deployment.StartArchitectureHandler"
            commandId="hu.bme.mit.incqueryd.tooling.deployment.commands.start">
      </handler>
   </extension>

</plugin>
