<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="hu.bme.mit.incqueryd.tooling.deployment.commands.install.full"
            name="Install IncQuery-D (Full)">
      </command>
      <command
            id="hu.bme.mit.incqueryd.tooling.deployment.commands.install.light"
            name="Install IncQuery-D (Light)">
      </command>
      <command
            id="hu.bme.mit.incqueryd.tooling.deployment.commands.deploy"
            name="Deploy IncQuery-D">
      </command>
      <command
            id="hu.bme.mit.incqueryd.tooling.deployment.commands.start"
            name="Start IncQuery-D">
      </command>
      <command
            id="hu.bme.mit.incqueryd.tooling.deployment.commands.destroy"
            name="Destroy IncQuery-D (coordinator and microkernels)">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
        <separator
		       name="separator-0"
		       visible="true">
		</separator>
         <command
               commandId="hu.bme.mit.incqueryd.tooling.deployment.commands.install.full"
               style="push">
         </command>
         <command
               commandId="hu.bme.mit.incqueryd.tooling.deployment.commands.install.light"
               style="push">
         </command>
        <separator
		       name="separator-1"
		       visible="true">
		</separator>
         <command
               commandId="hu.bme.mit.incqueryd.tooling.deployment.commands.deploy"
               style="push">
         </command>
         <command
               commandId="hu.bme.mit.incqueryd.tooling.deployment.commands.start"
               style="push">
         </command>
        <separator
		       name="separator-2"
		       visible="true">
		</separator>
         <command
               commandId="hu.bme.mit.incqueryd.tooling.deployment.commands.destroy"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="hu.bme.mit.incqueryd.tooling.ide.FullInstallArchitectureHandler"
            commandId="hu.bme.mit.incqueryd.tooling.deployment.commands.install.full">
      </handler>
      <handler
            class="hu.bme.mit.incqueryd.tooling.ide.LightInstallArchitectureHandler"
            commandId="hu.bme.mit.incqueryd.tooling.deployment.commands.install.light">
      </handler>
      <handler
            class="hu.bme.mit.incqueryd.tooling.ide.DeployArchitectureHandler"
            commandId="hu.bme.mit.incqueryd.tooling.deployment.commands.deploy">
      </handler>
      <handler
            class="hu.bme.mit.incqueryd.tooling.ide.StartArchitectureHandler"
            commandId="hu.bme.mit.incqueryd.tooling.deployment.commands.start">
      </handler>
      <handler
            class="hu.bme.mit.incqueryd.tooling.ide.DestroyArchitectureHandler"
            commandId="hu.bme.mit.incqueryd.tooling.deployment.commands.destroy">
      </handler>
   </extension>

</plugin>
