vocabulary <wikidata-demo-vocabulary.nt>

prefix wd: <http://www.wikidata.org/entity/> 
prefix wdt: <http://www.wikidata.org/prop/direct/>

pattern highestPoint(subject, object) {
	wdt:P610(subject, object);
}

pattern USMovieDirectors(movie, director) {
	wdt:P495(movie, wd:Q30);
	wdt:P57(movie, director);
}