package hu.bme.mit.train.constraintcheck.incquery

import "http://www.semanticweb.org/ontologies/2011/1/TrainRequirementOntology.owl" 

pattern switchSensor(Individual) = {
	Switch(Individual);
	neg find hasSensor(Individual);	
}

pattern hasSensor(Trackelement) = {
	Trackelement(Trackelement);
	Sensor(Target);
	Trackelement.TrackElement_sensor(Trackelement, Target);
}
